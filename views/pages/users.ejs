<% include ../partials/header %>

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
                <th>Password</th>
                <th><a class="btn btn-success" href="/usersadd" role="button">Add</a></th>
            </tr>
        </thead>
        <tbody>
        <% users.forEach(function(users){ %>
        <tr>
            <td scope="row"><%= users.id%></td>
            <td><%= users.email%></td>
            <td><%= users.password%></td>
            <td><a class="btn btn-success" href="/users/<%= users.id%>" role="button">Edit</a></td>
        </tr>
        <% }); %>
        </tbody>
    </table>                                                                        


<% include ../partials/footer %>